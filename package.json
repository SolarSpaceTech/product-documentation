{
  "name": "product-documentation",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run serve:dev-content & ng serve",
    "build": "npm run serve:dev-content & npm run init-routes && ng build && npm run stop:dev-content",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "serve:ssr:product-documentation": "node dist/product-documentation/server/server.mjs",
    "serve:dev-content": "npx ts-node --project tsconfig.scripts.json src/content-server/dev-content-server.ts",
    "stop:dev-content": "npx ts-node --project tsconfig.scripts.json src/content-server/stop-content-server.ts",
    "init-routes": "npx ts-node --project tsconfig.scripts.json scripts/init-prerender-routes.ts"
  },
  "engines": {
    "npm": ">=10.0.0 <11.0.0",
    "node": ">=22.0.0"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.2.4",
    "@angular/cdk": "^17.3.10",
    "@angular/common": "^18.2.4",
    "@angular/compiler": "^18.2.4",
    "@angular/core": "^18.2.4",
    "@angular/forms": "^18.2.4",
    "@angular/platform-browser": "^18.2.4",
    "@angular/platform-browser-dynamic": "^18.2.4",
    "@angular/platform-server": "^18.2.4",
    "@angular/router": "^18.2.4",
    "@angular/ssr": "^18.2.4",
    "@tinkoff/ng-polymorpheus": "^4.3.0",
    "bootstrap": "^4.6.2",
    "express": "^4.18.2",
    "fast-glob": "^3.3.2",
    "front-matter": "^4.0.2",
    "marked": "14.1.2",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.10"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.2.4",
    "@angular-eslint/eslint-plugin": "17.4.0",
    "@angular-eslint/eslint-plugin-template": "17.4.0",
    "@angular-eslint/template-parser": "17.4.0",
    "@angular/cli": "^18.2.4",
    "@angular/compiler-cli": "^18.2.4",
    "@types/express": "^4.17.21",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^18.18.0",
    "@typescript-eslint/eslint-plugin": "7.9.0",
    "@typescript-eslint/parser": "7.9.0",
    "cors": "^2.8.5",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jsdoc": "48.2.4",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-sonarjs": "1.0.3",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "remark-loader": "^6.0.0",
    "ts-node": "^10.9.2",
    "typescript": "~5.5.4"
  }
}
